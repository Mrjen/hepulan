/* pages/integralList/integralList.wxss */
.main{
	position: absolute;
	width: 100%;
	height: 100%;
	background:#f9f9f9;
}

.top_banner{
	width: 100%;
	height: 300rpx;
	background-size: 750rpx 354rpx;
	-webkit-box-shadow: 0 6px 9px 0 rgba(0,0,0,0.1) ;
  box-shadow: 0 6px 9px 0 rgba(0,0,0,0.1) ;
	border:1px solid transparent;
	background-image:url('data:image/png;base64,');
	line-height: 1;
	text-align: center;
	box-sizing: border-box;
}

.circle1{
	 width: 190rpx;
	 height: 190rpx;
	 border-radius: 50%;
	 background: rgba(17, 204, 153, 0.3);
	 display: flex;
	 justify-content: center;
	 align-items: center;
	 margin:0 auto;
	 margin-top: 50rpx;
}

.circle2{
	 width: 160rpx;
	 height: 160rpx;
	 border-radius: 50%;
	 background: rgba(17, 204, 153, 0.5);
	 display: flex;
	 justify-content: center;
	 align-items: center;
}

.circle2 image{
	width:134rpx;
	height: 134rpx;
	border-radius: 50%;
}

.banner_title{
	font-size: 40rpx;
	font-weight: bold;
}

.mine_list{
	text-align: right;
	font-size: 30rpx;
}

.list_nav{
	display: flex;
	height: 106rpx;
	line-height: 106rpx;
	font-size: 40rpx;
	text-align: center;
	background:#fff;
	position: relative;
}

.list_nav:before {
  content: "";
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  border-top: 1px solid #ddd;
  -webkit-transform: scaleY(.5);
  -webkit-transform-origin: 0 0;
  pointer-events: none;
}

.list_nav view{
	flex:1;
}

.list_nav view:nth-of-type(2){
	flex: 2;
}

.persion_list{
	display: flex;
	height: 130rpx;
	background:#fff;
	box-sizing: border-box;
	justify-content: flex-start;
	align-items: center;
	position: relative;
	margin: 24rpx 0;
	padding:0 40rpx;
	font-size: 26rpx;
	-webkit-box-shadow: 0 0 6px 2px rgba(0,0,0,0.1) ;
  box-shadow: 0 0 6px 2px rgba(0,0,0,0.1) ;
}

.persion_list view:nth-of-type(1){
	width: 100rpx;
	text-align: center;
	margin-right: 30rpx;
}

.persion_list image{
	display: block;
	width: 88rpx;
	height: 88rpx;
	border-radius: 50%;
	margin-right: 30rpx;
}

.persion_list view:nth-of-type(2){
	width: 300rpx;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}

.persion_list view:nth-of-type(3){
	flex: 1;
}

.persion_list view:nth-of-type(4){
	text-align: right;
	font-size: 26rpx;
}

.gold_box{
	display: flex;
	justify-content: space-between;
	box-sizing: border-box;
	padding: 0 40rpx;
	font-size: 26rpx;
}

.helpCoins{
  display: inline-block;
  width: 30rpx;
  height: 30rpx;
  font-size: 30rpx;
  color: #999;
  border:1px solid #999;
  line-height: 34rpx;
  text-align: center;
  border-radius: 50%;
  vertical-align: top;
  margin-left: 10rpx;
}


/*弹窗*/
.help_win{
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
  position: fixed;
  left: 0;
  top: 0;
  display: flex;
  justify-content: center;
  align-items: center;
}

.help_win_in{
  width: 608rpx;
  height: 708rpx;
  background: #fff;
  border-radius: 6rpx;
}

.help_title{
  background: #75c0ac;
  color: #fff;
  font-size: 40rpx;
  text-align: center;
  line-height: 100rpx;
  height: 100rpx;
  border-radius: 6rpx 6rpx 0 0;
  position: relative;
  margin-bottom: 44rpx;
}

.help_title .close_help{
  display: block;
  position: absolute;
  content: "";
  width: 60rpx;
  height: 60rpx;
  background-size: 30rpx 30rpx;
  background-position: center center;
  background-repeat: no-repeat;
  background-image:url('data:image/png;base64,');
  right:10rpx;
  top:20rpx;
}

.help_content{
  width: 520rpx;
  margin:0 auto;
  font-size: 31rpx;
  line-height: 50rpx;
  text-align: justify;
}

.help_content view{
  display: flex;
  margin-bottom: 72rpx;
}

.help_content view:before{
  content: "";
  display: inline-block;
  width: 24rpx;
  height: 24rpx;
  border-radius: 50%;
  background: #75c0ac;
  flex-shrink: 0;
  margin-top: 13rpx;
  margin-right: 14rpx;
  vertical-align: middle;
}
