/* pages/heChart/heChart.wxss */
.container{
  padding:0;
  margin:0;
  background: #f8f9fb;
}

.top_nav{
  width: 750rpx;
  height: 100rpx;
  display: flex;
  position: relative;
  background-color: #fff;
}

.top_nav view:nth-of-type(1){
  width: 25%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.top_nav::after{
  display: block;
  content: "";
  width: 750rpx;
  position: absolute;
  left: 0;
  bottom: 0;
  -webkit-transform: scaleY(.5);
  -webkit-transform-origin: 0 0;
  pointer-events: none;
}

.top_nav_keyworld  view{
  display: flex;
  flex:1;
  align-items: center;
  justify-content: center;
}

.top_nav_keyworld{
  width: 75%;
  flex:1;
  display: flex;
}

.top_nav_keyworld view:nth-of-type(1):before{
  content: "";
  display: block;
  width: 50rpx;
  height: 50rpx;
  margin-right: 8rpx;
  background-image:url('data:image/png;base64,');
  background-size: 50rpx;
}

.top_nav_keyworld view:nth-of-type(2):before{
  content: "";
  display: block;
  width: 50rpx;
  height: 50rpx;
  margin-right: 8rpx;
  background-image:url('data:image/png;base64,');
  background-size: 50rpx;
}

.top_nav_keyworld view:nth-of-type(3):before{
  content: "";
  display: block;
  width: 50rpx;
  height: 50rpx;
  margin-right: 8rpx;
  background-image:url('data:image/png;base64,');
  background-size: 50rpx;
}

.top_nav image{
  width: 104rpx;
  height: 40rpx;
  display: block;
  margin:0 auto;
  position: relative;
}

/*轮播*/
.swiper{
  width: 750rpx;
  height: 416rpx;
}

.swiper swiper-item image{
  display: block;
  width: 100%;
  height: 100%;
}

.img_tip{
  position: absolute;
  width: 750rpx;
  height: 100rpx;
  background: rgba(0, 0, 0, 0.2);
  margin-top: -100rpx;
  color: #fff;
  font-size: 30rpx;
  box-sizing: border-box;
  padding:10rpx;
  text-align: center;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

/*主体内容*/
.tag_scroll{
  width: 750rpx;
  height: 90rpx;
  display: block;
}

.label_nav{
  width: 750rpx;
  height: 88rpx;
  background-color: #fff;
  border-bottom:1px solid #eee;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: flex-start;
  color: #333;
}

.label_item{
  display: inline-block;
  box-sizing: border-box;
  flex-shrink: 0;
  padding:0 26rpx;
  height: 88rpx;
  font-size: 30rpx;
  line-height: 88rpx;
}

.label_nav .active{
  border-bottom:2px solid #0fc191;
  color: #0fc191;
}

/*视频列表*/
.video_item{
  width: 750rpx;
  height: 420rpx;
  display: flex;
  justify-content: space-between;
  flex-direction: column;
  background-size: 100% 100%;
  box-sizing: border-box;
}

.video_describe{
  display: flex;
  justify-content: space-between;
  box-sizing: border-box;
  padding:0 20rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 28rpx;
  background-color: rgba(0, 0, 0, 0.2);
  color: #fff;
  position: absolute;
  bottom: 130rpx;
  border-radius: 30rpx;
  right: 50rpx;
}

.video_describe view:nth-of-type(2){
  color: #ffff00;
}

.video_static{
  width: 140rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  font-size: 26rpx;
  color: #ffff00;
  align-self: flex-end;
  border-radius: 0 0 0 10rpx;
  background-color: rgba(0, 0, 0, 0.5);
}

.video_title{
  width: 750rpx;
  min-height: 60rpx;
  line-height: 32rpx;
  font-size: 32rpx;
  max-height: 120rpx;
  color: #a4a5a7;
  box-sizing: border-box;
  padding:16rpx 20rpx 0 20rpx;
  background-color: #fff;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  letter-spacing: 3rpx;
}

.video_box{
  position: relative;
  padding-bottom: 36rpx;
  -webkit-box-shadow: 1px 1px 4px 0 rgba(0,0,0,0.2) ;
  box-shadow: 1px 1px 4px 0 rgba(0,0,0,0.2) ;
}
