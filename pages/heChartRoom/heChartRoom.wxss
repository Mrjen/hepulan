/* pages/heChartRoom/heChartRoom.wxss */
@import "../../utils/wxParse.wxss";
.container{
  padding:0;
  margin:0;
  position: absolute;
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
}

.scroll_box{
   width: 100%;
   flex: 1;
   box-sizing: border-box;
   position: relative;
   height: 100vh;
   background: #eee;
}

.video_list{
  background: #eee;
}

/*PTT*/
.ppt{
  width: 750rpx;
  height: 0;
  background-color: #eee;
}

.ppt swiper{
  width: 100%;
  height: 100%;
  overflow: hidden;
}

.ppt swiper-item image{
  width: 100%;
  height: 100%;
}

.ppt_dots{
  width: 30px;
  height: 16px;
  border-radius: 18px;
  border:1px solid #fff;
  margin-top: -60rpx;
  position: relative;
  z-index: 10;
  text-align: center;
  color: #fff;
  font-size: 26rpx;
  line-height: 16px;
  margin-left: 30rpx;
}

/*弹屏*/
.barrage_box{
   width: 500rpx;
   overflow-y: scroll;
   position: fixed;
   top: 110rpx;
   right: 30rpx;
   color: #fff;
   box-sizing: border-box;
   background: rgba(0, 0, 0, 0.1);
   border-radius: 8rpx;
}

.barrage_item{
  display: flex;
  justify-content: flex-end;
  width: 100%;
}

.barrage_persion{
  width: 50rpx;
  height: 50rpx;
  flex-shrink: 0;
  border-radius: 50%;
  margin:20rpx 14rpx 14rpx 20rpx;
}

.barrage_content{
  background: rgba(0, 0, 0, 0.3);
  font-size: 32rpx;
  border-radius: 6rpx;
  margin:8rpx 0;
  line-height: 68rpx;
  margin-left: 20rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}

.barrage_item:nth-of-type(1){
  margin-top: 14rpx;
}

.barrage_item:nth-of-type(3){
  margin-bottom: 14rpx;
}

.barrage_content image{
  display: inline-block;
  width: 50rpx;
  height: 50rpx;
  background: transparent;
}

/*页面主体*/

.teacher_box{
  width: 270rpx;
  height: 60rpx;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  padding:0 20rpx;
  border-radius: 40rpx;
  background: rgba(0, 0, 0, 0.3);
  /*background-image:url('data:image/png;base64,');*/
  background-repeat: no-repeat;
  background-position: 240rpx center;
  background-size: 18rpx 36rpx;
}

.teacher_box image{
  width: 48rpx;
  height: 48rpx;
  margin-left: -10rpx;
  flex-shrink: 0;
  border-radius: 50%;
  border:1px solid #fff;
  display: inline-block;
}

.pop_nav{
  width: 350rpx;
  height: 60rpx;
  display: flex;
  align-items: center;
  line-height: 60rpx;
  color: #fff;
  font-size: 28rpx;
  border-radius: 60rpx;
  background: rgba(0, 0, 0, 0.5);
  overflow: hidden;
  transform-origin: 100% 100%;
}

.pop_nav_out{
  overflow: hidden;
  transform-origin: 100% 100%;
}

.persion_pop::after{
  content: "";
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  display: inline-block;
  width: 30rpx;
  height: 26rpx;
  flex-shrink: 0;
  background-image:url('data:image/png;base64,');
  background-size: 30rpx 26rpx;
  margin:0 10rpx;
}

.edit_btn{
  width: 60rpx;
  height: 60rpx;
  flex-shrink: 0;
  border-radius: 80rpx;
  background-color: #09bb07;
  background-image:url('data:image/png;base64,');
  background-size: 26rpx 24rpx;
  background-repeat: no-repeat;
  background-position: center;
  margin-right: 10rpx;
}

.persion_opation{
  display: flex;
  justify-content: space-between;
  box-sizing: border-box;
  padding:20rpx;
  position: fixed;
  width: 100%;
  top: 20rpx;
  z-index: 10;
}

.barrage_btn{
  width: 350rpx;
  overflow: hidden;
}

.close_pop{
  width: 60rpx;
  height: 60rpx;
  border-radius: 80rpx;
  background: rgba(178, 178, 178, 0.8);
  color: #fff;
  text-align: center;
  line-height: 60rpx;
  font-size: 28rpx;
  position: absolute;
  top: 20rpx;
  right: 20rpx;
  z-index: 11;
}

.opation_box{
  position: fixed;
  top: 150rpx;
  right: 0;
  z-index: 10;
}

.opation_box view{
  width: 104rpx;
  height: 74rpx;
  font-size: 30rpx;
  line-height: 74rpx;
  text-align: center;
  color: #fff;
  background: rgba(0, 0, 0, 0.5);
}

.opation_box view:nth-of-type(1){
  border-radius: 10rpx 0 0 0;
  margin-bottom: 1px;
}

.opation_box view:nth-of-type(2){
  border-radius: 0;
  margin-bottom: 1px;
}

.opation_box view:nth-of-type(3){
  border-radius: 0 0 0 10rpx;
  margin-bottom: 1px;
}

/*老师聊天记录*/
.main_content{
  width: 750rpx;
  height: 100%;
  position: absolute;
  word-break: break-all;
  box-sizing: border-box;
  background: #eee;
  padding:0 30rpx;
}

.teach_img{
  flex-shrink: 0;
  margin-right: 40rpx;
}

.teach_img image{
  width: 60rpx;
  height: 60rpx;
  margin-top: 46rpx;
  border-radius: 10rpx;
}

.chart_item{
  display: flex;
  overflow: hidden;
}

.chart_item:last-of-type{
  margin-bottom: 250rpx;
}

.persion_content{
  display: inline-block;
  background: #fff;
  box-sizing: border-box;
  text-align: justify;
  padding:20rpx;
  font-size: 36rpx;
  line-height: 48rpx;
  border-radius: 10rpx;
  position: relative;
  max-width: 460rpx;
  margin-bottom: 20rpx;
}

.persion_content::before{
  content: "";
  display: block;
  width: 20rpx;
  height: 20rpx;
  position: absolute;
  margin-left: -28rpx;
  margin-top: 10rpx;
  transform: rotate(45deg);
  background-color: #fff;
}

.audio_chart{
  /*border: 1px solid red;*/
  height: 82rpx;
  display: flex;
  min-width: 120rpx;
  background: #fff;
  border-radius: 10rpx;
  position: relative;
  box-sizing: border-box;
  padding:20rpx;
  margin-bottom: 20rpx;
  overflow: initial;
}

.audio_chart::before{
  content: "";
  display: block;
  width: 20rpx;
  height: 20rpx;
  margin-left: -28rpx;
  margin-top: 14rpx;
  position: absolute;
  transform: rotate(45deg);
  background: #fff;
}

.teach_name{
  width: 100%;
  font-size: 28rpx;
 margin-bottom: 4rpx;
}

.teach_time{
  width: 460rpx;
  font-size: 24rpx;
  text-align: center;
  color: #999;
}

.audio_len{
  display: inline-flex;
  flex-shrink: 0;
  line-height: 82rpx;
  margin-left: 20rpx;
  color: #666;
  font-weight: bold;
  font-size: 26rpx;
  justify-content:flex-end;
  flex-direction:column;
}

.myAudio{
  display: block;
  width: 100%;
  height: 100%;
}

.chart_content{
  display: flex;
}

.content_img{
  width: 400rpx;
}

.play_bg{
  width: 40rpx;
  height: 50rpx;
  box-sizing: border-box;
  background-size: 120rpx 50rpx;
  background-image: url(https://hepulan.playonwechat.com/static/heChart/yuying.png);
  background-position: -80rpx 0;
}

.play_audio{
  width: 40rpx;
  height: 50rpx;
  box-sizing: border-box;
  background-size: 120rpx 50rpx;
  background-image: url(https://hepulan.playonwechat.com/static/heChart/yuying.png);
  animation: animate_voice 1.5s steps(3) infinite;
  -webkit-animation: animate_voice 1.5s steps(3) infinite;
}

@keyframes animate_voice {
  0% {
     background-position: 0 0;
   }
   100% {
     background-position: -120rpx 0;
   }
}

@-webkit-keyframes animate_voice {
  0% {
     background-position: 0 0;
   }
   100% {
     background-position: -120rpx 0;
   }
}

/*讨论区*/
.bottom_nav{
  width: 750rpx;
  height: 84rpx;
  background: #09bb07;
  color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  left: 0;
  bottom: 0;
  font-size: 34rpx;
}

.discuss{
  flex: 1;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.discuss view{
  width: 34rpx;
  height: 34rpx;
  margin:0 20rpx;
  display: inline-block;
  background-image:url('data:image/png;base64,');
  background-size: 34rpx 34rpx;
}

.discuss_win{
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  background: rgba(0, 0, 0, 0.5);
  z-index: 20;
}

.callback_btn{
  width: 200rpx;
  height: 74rpx;
  text-align: center;
  color: #fff;
  font-size: 30rpx;
  line-height: 74rpx;
  margin:20rpx auto;
  background: #00b700;
  border:1px solid #10ad0e;
  border-radius: 10rpx;
}

.dis_pers_img{
  display: block;
  width: 84rpx;
  height: 84rpx;
  border-radius: 50%;
  flex-shrink: 0;
  margin-right: 14rpx;
}

.discuss_item{
  display: flex;
  box-sizing: border-box;
  padding: 20rpx;
  position: relative;
}

.discuss_item:last-child{
  margin-bottom: 280rpx;
}

.discuss_item:before {
  content: "";
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  border-bottom: 1px solid #e5e5e5;
  -webkit-transform: scaleY(.5);
  -webkit-transform-origin: 0 0;
  pointer-events: none;
}

.discuss_item .dis_cussname{
  color: #00b700;
  font-size: 28rpx;
  margin-bottom: 20rpx;
}

.discuss_item .discuss_cotent_item  view:nth-of-type(2){
  color: #727272;
  font-size: 26rpx;
  margin-bottom: 20rpx;
}

.discuss_item .discuss_cotent_item  view:nth-of-type(3){
  color: #333;
  font-size: 34rpx;
  margin-bottom: 20rpx;
}

.discuss_main{
  background: #fff;
  height: 1206rpx;
}

.disscuss_write{
  width: 750rpx;
  height: 104rpx;
  position: fixed;
  left: 0;
  bottom: 0;
  background:#fff;
  display: flex;
  align-items: center;
  -webkit-box-shadow: 1px 1px 4px 0 rgba(0,0,0,0.2) ;
  box-shadow: 1px 1px 4px 0 rgba(0,0,0,0.2) ;
}

.write_btn{
  width: 710rpx;
  background: #f8f8f8;
  line-height: 84rpx;
  color: #9b9b9b;
  margin:0 auto;
  border:1px solid #dfdfdf;
  border-radius: 8rpx;
  font-size: 34rpx;
  box-sizing: border-box;
  padding: 0 20rpx;
  display: flex;
  align-items: center;
}

.write_icon{
  width: 36rpx;
  height: 34rpx;
  background-image:url('data:image/png;base64,');
  background-size: 36rpx 34rpx;
  display: inline-block;
  margin-right: 20rpx;
}

.write_box{
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  background: rgba(0, 0, 0,.5);
  z-index: 22;
  display: flex;
  flex-direction: column-reverse;

}

.write_box_in{
  background: #fff;
  width: 750rpx;
  overflow: hidden;
  left: 0;
  bottom: 0;
  right: 0;
}

.write_opation{
  display: flex;
  justify-content: space-between;
  box-sizing: border-box;
  padding: 0 20rpx;
}

.opation_btn{
  padding: 6rpx 10rpx;
  margin: 24rpx 0;
}

.opation_btn:nth-of-type(1){
  color: #7f7f7f;
}

.opation_btn:nth-of-type(2){
  color: #00b100;
}

.write_input{
  width: 710rpx;
  height: 210rpx;
  background: #f8f8f8;
  border:1px solid #e7e7e7;
  border-radius: 8rpx;
  margin: 0 auto;
  margin-bottom: 20rpx;
  box-sizing: border-box;
  padding: 20rpx;
  font-size: 34rpx;
  display: block;
}

.radio{
  box-sizing: border-box;
  padding: 20rpx 20rpx;
  display: flex;
  align-items: center;
}

.radio_radio{
  display: inline-block;
  width: 24px;
  height: 24px;
  border-radius: 50%;
  border:1px solid #c0c0c0;
  display: flex;
  justify-content: center;
  align-items: center;
}

.radio_radio view{
  width: 14px;
  height: 14px;
  border-radius: 50%;
  background: #00ab00;
}

.switch_emoji{
  width: 44rpx;
  height: 44rpx;
  background-image:url('data:image/png;base64,');
  background-size:44rpx 44rpx;
}

.switch_box{
  display: flex;
  align-items:center;
  justify-content:flex-end;
  box-sizing: border-box;
  padding: 20rpx;
}

.emoji_box{
  box-sizing: border-box;
  padding:20rpx;
}

.emoji_box image{
  width: 44rpx;
  height: 44rpx;
  margin:4rpx 12rpx;
  display: inline-block;
}

.course_static{
  width: 560rpx;
  height: 60rpx;
  background: #fff;
  border-radius: 10rpx;
  color: #999;
  text-align: center;
  line-height: 60rpx;
  margin:20rpx auto;
  font-size: 26rpx;
}

.ask_name{
  position: relative;
  box-sizing:border-box;
  padding-bottom:10rpx;
}

.ask_name::before{
  content: "问";
  display: inline-block;
  width: 30rpx;
  height: 30rpx;
  border:1px solid #00b100;
  font-size: 28rpx;
  line-height: 30rpx;
  text-align: center;
  border-radius: 6rpx;
  color: #00b100;
  position: relative;
  top: 50%;
  margin-top: -15rpx;
}

.ask_name::after{
  content: "";
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  border-bottom: 1px solid #858585;
  -webkit-transform: scaleY(.5);
  -webkit-transform-origin: 0 0;
  pointer-events: none;
}

.quest{
  color: #858585;
}

.pers_content{
  font-size: 34rpx;
}

.pers_content image{
  display: inline-block;
  width: 44rpx;
  height: 44rpx;
  background-color: transparent;
}
