/* pages/mine/mine.wxss */
.container{
	padding:0;
	margin: 0;
	font-size: 30rpx;
	line-height: 1;
	background:#417374;
	position: absolute;
	width: 100%;
	height: 100%;
	display: flex;
	justify-content: flex-start;
}

.userinfo{
    width: 750rpx;
    height: 512rpx;
    background-size:750rpx 512rpx;
    box-sizing: border-box;
    position: relative;
    background-image:url('data:image/png;base64,');
}

.userinfo .userimg{
	width: 130rpx;
	height: 130rpx;
	border-radius: 50%;
	display: block;
}

.user_circle1{
  width: 220rpx;
  height: 220rpx;
  margin: 30rpx auto;
  background: rgba(17,204,153,.2);
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.user_circle2{
  width: 190rpx;
  height: 190rpx;
  background: rgba(17,204,153,.5);
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.user_circle3{
  width: 160rpx;
  height: 160rpx;
  background: rgba(17,204,153,.8);
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.userinfo .user_name{
	font-size: 40rpx;
	line-height: 70rpx;
	color: #333;
  text-align: center;
}

.userinfo .user_tag{
	font-size: 30rpx;
	line-height: 36rpx;
	color: #888;
  text-align: center;
}

.opation_list image{
	width: 410rpx;
	height: 109rpx;
	display: block;
  margin: 20rpx auto;
}

.opation_list{
	width: 392rpx;
	height: 90rpx;
	line-height: 90rpx;
	border-radius: 45rpx;
	margin: 30rpx auto;
	font-size: 34rpx;
	position: relative;
	text-align: center;
	box-sizing: content-box;
    cursor: pointer;
    outline: none;
    outline-color: transparent;
    color:#fff;
    background: -webkit-linear-gradient(-90deg, #10ca98 0, #0ba178 100%);
    background: linear-gradient(180deg, #10ca98 0, #0ba178 100%);
    background-position: 50% 50%;
    -webkit-box-shadow: 0 1px 8px 2px rgba(0,0,0,0.2);
}

.opation_list_box{
    margin: 0;
    width: 100%;
    overflow: hidden;
    position: relative;
    box-sizing: border-box;
    margin-bottom: 28rpx;
}
