/* pages/heChartTeach/heChartTeach.wxss */
.teacher_img{
  display: block;
  width: 750rpx;
  height: 420rpx;
}

.video_title{
  text-align: left;
  width: 100%;
  font-weight: bold;
  box-sizing: border-box;
  padding:16rpx 20rpx;
  border-bottom: 1px solid #efefef;
}

.main_box{
  width: 100%;
  box-sizing: border-box;
}

.bold_text{
  color:#8b969c;
  font-size: 32rpx;
  font-weight: bold;
  display: inline-block;
}

.teach_room{
  display: inline-block;
  color: #0095ff;
  font-size: 26rpx;
  box-sizing: border-box;
  padding:0 22rpx;
}

.follow{
  display: inline-block;
  font-size: 26rpx;
  box-sizing: border-box;
  padding:14rpx 20rpx;
  background-color: #ddf4ff;
  border-radius: 6rpx;
  color: #0095ff;
}

.bold_text2{
  color:#8b969c;
  font-size: 32rpx;
  font-weight: bold;
  display:block;
}

.room_itle{
  width: 100%;
  box-sizing: border-box;
  padding:44rpx 20rpx 0 20rpx;
}

.teach_name{
   font-size: 28rpx;
   color: #333;
   display: inline-block;
}

.teach_introduct{
   font-size: 28rpx;
   color: #828f95;
}

.introduct{
  margin: 68rpx auto 28rpx auto;
  color: #333;
  font-weight: bold;
}

.countDown{
  margin-top: 40rpx;
}

.count_down{
  font-size: 26rpx;
  color: #989898;
  display: inline-block;
  vertical-align: bottom;
}

.count_down::before{
  content: "";
  display: inline-block;
  width: 26rpx;
  height: 26rpx;
  margin-right: 8rpx;
  vertical-align: top;
  background-image:url('data:image/png;base64,');
  background-size: 100% 100%;
}

.item_tip{
  box-sizing: border-box;
  padding: 0 20rpx;
}

.time_item{
  display: inline-block;
  background-color: #09bb07;
  border:1px solid #00b500;
  color: #fff;
  width: 104rpx;
  height: 62rpx;
  font-size: 34rpx;
  border-radius: 6rpx;
  margin: 0 6rpx;
  line-height: 62rpx;
  text-align: center;
  box-sizing: border-box;
  font-weight: bold;
}

.time_item text{
  font-size: 24rpx;
  font-weight: normal;
}

.time_box{
  display: inline-block;
}

.sign_box{

}

.sign_box text{
    color: #7e8b91;
    font-size: 26rpx;
    display: inline-block;
    margin-left: 16rpx;
}

.persion_img image{
  width: 54rpx;
  height: 54rpx;
  border-radius: 50%;
  margin:0 6rpx;
  display: inline-block;
}

.more_persion{
  width: 54rpx;
  height: 54rpx;
  display: inline-block;
  border-radius: 50%;
  background-size: 100% 100%;
  margin-left: 3rpx;
  background-image:url('data:image/png;base64,');
}

.has_sign{
  color: #333;
  margin: 40rpx auto;
}

.sign_box{
  margin: 50rpx auto 40rpx auto;
  border-top:1px solid #f6f6f6;
  border-bottom:1px solid #f6f6f6;
  box-sizing: border-box;
  padding:0 20rpx 20rpx 20rpx;
}

.course{
  box-sizing: border-box;
  padding:0 20rpx;
}

.course .bold_text{
  color: #333;
  margin-bottom: 30rpx;
}

.course_intro{
  font-size: 30rpx;
  color: #7f8c92;
  margin-bottom: 150rpx;
}

.navto{
  width: 100%;
  height: 100rpx;
  background: #09bb07;
  color: #fff;
  line-height: 100rpx;
  text-align: center;
  position: fixed;
  left: 0;
  bottom: 0;
}
