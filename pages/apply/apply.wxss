.container{
  padding: 0;
  margin: 0;
  text-align: center;
}

.join_banner {
  width: 750rpx;
}

.join_banner image {
  width: 750rpx;
  -webkit-box-shadow:0px 4px 30px rgba(0,0,0,.5);
  box-shadow:0px 4px 30px rgba(0,0,0,.5);
}

.join_suit {
  width: 750rpx;
  margin-top: 30rpx;
  box-sizing: border-box;
}

.join_suit .suit {
  width: 458rpx;
  height: 300rpx;
  border-radius: 10px;
  -webkit-box-shadow:0px 4px 13px rgba(0,0,0,.5);
  box-shadow:0px 4px 13px rgba(0,0,0,.5);
}

.product_item{
  display: flex;
  justify-content: space-between;
  width: 702rpx;
  height: 242rpx;
  box-sizing: border-box;
  padding-right: 45rpx;
  margin:0 auto;
  margin-bottom: 26rpx;
  align-items: center;
  -webkit-box-shadow: 0 0 8px 0 rgba(0,0,0,0.5) ;
  box-shadow: 0 0 8px 0 rgba(0,0,0,0.5) ;
}

.product_img image{
  width: 354rpx;
  height: 216rpx;
  display: block;
  margin-left: 12rpx;
}

.product_title{
  font-size: 26rpx;
}

.apply_btn{
  font-size: 26rpx;
  display: inline-block;
  box-sizing: border-box;
  border: 1px solid #000;
  padding:8rpx;
}

.product_eng{
  font-size: 20rpx;
  margin-bottom: 36rpx;
}

.product_info{
  text-align: right;
}

.join_suit .qrcode {
  width: 220rpx;
  height: 220rpx;
}

button::after{
  border:none;
}

.join_btns {
  width: 284rpx;
  height: 64rpx;
  border:none;
  margin-bottom: 200rpx;
  background-image:url('data:image/png;base64,');
  background-size: 100% 100%;
}

.join_btns.other-button-hover {
  border:none;
  background-image:url('data:image/png;base64,');
}

.teaacher_tip {
  text-align: center;
  color: #999;
  font-size: 24rpx;
  margin-bottom: 220rpx;
}

.applyWin {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
  left: 0;
  top: 0;
  position: fixed;
  display: flex;
  justify-content: center;
  align-items: center;
}

.applyIn {
  width: 580rpx;
  height: 314rpx;
  background: #fff;
  border-radius: 20rpx;
  box-sizing: border-box;
  padding: 20rpx 40rpx;
  text-align: center;
}

.winTitle {
  margin: 30rpx;
}

.closeWin {
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  border: 1px solid #fff;
}

.teacherCode {
  margin: 20rpx 0;
  text-align: center;
  display: inline-block;
  padding: 10rpx 20rpx;
  background: #365742;
  color: #fff;
}

.closeWin {
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  border: 1px solid #000;
  position: absolute;
  right: 115rpx;
}

.closeWin:before {
  content: "";
  display: block;
  width: 36rpx;
  height: 2rpx;
  background: #000;
  transform: rotate(45deg);
  position: absolute;
  margin-top: -2rpx;
  top: 50%;
  left: 50%;
  margin-left: -16rpx;
}

.closeWin:after {
  content: "";
  display: block;
  width: 36rpx;
  height: 2rpx;
  background: #000;
  transform: rotate(-45deg);
  top: 50%;
  left: 50%;
  margin-left: -16rpx;
  margin-top: -2rpx;
  position: absolute;
}
