/**index.wxss**/
@import '../moreTeacher/moreTeacher.wxss';

view{
    box-sizing: border-box;
}

page{
    width: 750rpx;
    overflow-x: hidden;
}

.container{
  padding-bottom: 80rpx;
}

.banner{
    width: 750rpx;
    height: 300rpx;
    background-size: 100% 100%;
    display: flex;
    align-items: center;
    justify-content: center;
}

.banner swiper-item image{
    width: 100%;
    height: 100%;
    display: block;
    position: relative;
    z-index: 1;
}

.banner .ask-btn{
    width: 182rpx;
    height: 62rpx;
    /* background-image: url('http://p1jrmxejh.bkt.clouddn.com/hepulanhufu/ask-btn-bg.png'); */
    background-size: 100% 100%;
    background-color: transparent;
    padding:0;
    border-radius: 0;
    position: absolute;
    z-index: 2;
    left: 50%;
    transform: translateX(-50%);
    top: 120rpx;
}

.search-view{
    width: 750rpx;
    height: 80rpx;
    background: #006d4a;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding:0 20rpx;
}

.search-view .search{
    width: 597rpx;
    height: 54rpx;
    display: block;
    background:#fff;
    border-radius: 6rpx;
    font-size: 27rpx;
    line-height: 54rpx;
    padding-left: 20rpx;
    color: #999;
    border:none;
    background-size: 100% 100%;
    display: flex;
    align-items: center;
}

.search::before{
    content:' ';
    display: block;
    width: 28rpx;
    height: 28rpx;
    background: url('data:image/png;base64,');
    background-size: 100% 100%;
    margin-right: 10rpx;
}

.search-view text{
    color: #fff;
    font-size: 28rpx;
    display: inline-block;
    margin-right: 16rpx;
}

.top-tip{
    width: 750rpx;
    height: 60rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding-right: 20rpx;
}

.top-tip .xian{
    width: 570rpx;
    height: 1px;
    background: #006d4a;
    position: relative;
    display: flex;
    align-items: center;
}

.top-tip .xian::after{
    content:" ";
    display: block;
    width: 14rpx;
    height: 14rpx;
    background: #006d4a;
    border-radius: 50%;
    position: absolute;
    right: 0;
    top: 50%;
    transform: translateY(-50%);
}

.top-tip navigator{
    font-size: 26rpx;
    color: #666;
}

/* 老师轮播 */
.teacher-swiper{
    width: 750rpx;
}

.teacher-banner{
    height: 290rpx;
}

.teacher-banner image{
    display: block;
    width: 100%;
    height: 100%;
}

/* 功能按钮 */
.btn-view{
    display: flex;
    height: 80rpx;
    justify-content: space-around;
}

.btn-view navigator{
    width: 164rpx;
    height: 44rpx;
    background: #006d4a;
    color: #fff;
    font-size: 24rpx;
    line-height: 44rpx;
    text-align: center;
    border-radius: 50rpx;
    margin: 30rpx 0 40rpx 0;
    box-shadow: 0 0 4rpx #006d4a;
}

/* bottom-tip */
.bottom-tip{
  width: 750rpx;
    height: 60rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-top: -10rpx;
}

.bottom-tip .xian{
    width: 670rpx;
    height: 1px;
    background: #006d4a;
    position: relative;
    display: flex;
    align-items: center;
    justify-content: flex-end;
    right: 0;
}

.bottom-tip .xian::before{
    content: "";
    display: block;
    width: 14rpx;
    height: 14rpx;
    background: #006d4a;
    border-radius: 50%;
    position: absolute;
    left: 0;
    top: 50%;
   transform: translateY(-50%);
}

.bottom-tip navigator{
    font-size: 22rpx;
    color: #999;
    width: 100rpx;
}

/* 视频区域 */
.video-view .text{
    text-align: center;
    color: #006d4a;
}
.text01{
   font-size: 22rpx;
   font-weight: bold;
}

.text02{
    font-size: 34rpx;
    font-weight: bold;
    margin:20rpx auto;
}

.text03{
    font-size: 22rpx;
    color: #666 !important;
}

.zuge-view{
  width: 750rpx;
  overflow: hidden;
}

.zuge{
    width: 14rpx;
    height: 14rpx;
    background: #006d4a;
    border-radius: 50%;
    margin:0 auto;
    position: relative;
    display: flex;
    align-items: center;
    margin-top: 40rpx;
    justify-content: space-between;
}

.zuge::before{
    content:' ';
    width: 360rpx;
    height: 1px;
    background: #006d4a;
     position: absolute;
    left: 30rpx; 
    top: 50%;
    transform: translateY(-50%)
}

.zuge::after{
    content:' ';
    width: 360rpx;
    height: 1px;
    background: #006d4a;
     position: absolute;
    right:30rpx; 
    top: 50%;
    transform: translateY(-50%)
}

.video-tip{
    text-align: center;
    font-size: 22rpx;
    margin:10rpx auto;
}

.video-tip text{
    color: red;
}

.video-view .video{
    display: flex;
    justify-content: center;
    align-items: center;
}

.video-cover{
    width: 518rpx;
    height: 310rpx;
    border-radius: 20rpx;
}

.video-contact {
    width: 195rpx;
    height: 45rpx;
    background: #006d4a;
    color:#fff;
    border-radius: 0;
    display: flex;
    font-size: 26rpx;
    align-items: center;
    justify-content: center;
    padding: 0;
    box-sizing: border-box;
    margin: 20rpx auto;
}

.video-contact text{
    display: block;
    width: 14rpx;
    height: 17rpx;
    background: url('data:image/png;base64,');
    background-size: 100% 100%;
    margin-right: 10rpx;
}



