/* 红包 */
.red-pack {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, .5);
    display: none;
    justify-content: center;
    align-items: center;
    z-index: 100;
}

.pack-inner {
    width: 580rpx;
    height: 775rpx;
    box-sizing: border-box;
    padding: 20rpx;
    position: relative;
    background-size: 100% 100%;
    background-image: url('https://qncdn.playonwechat.com/hepulanhufu/red-pack-bg.png');
    animation: bounceIn 1s 0s forwards;
}

.red-logo {
    width: 80rpx;
    height: 80rpx;
    display: block;
    margin: 50rpx auto 24rpx auto;
}

.red-title {
    text-align: center;
    color: #e6c996;
    margin: 8rpx 0;
    font-size: 24rpx;
}

.red-content {
    text-align: center;
    font-size: 36rpx;
    color: #e6c996;
	margin-top: 78rpx;
	margin-bottom: 48rpx;
}

.open-btn {
    width: 152rpx;
    height: 152rpx;
	background-image: url('https://qncdn.playonwechat.com/hepulanhufu/red-pack-open-btn.png'); 
	background-size: 100% 100%;
    margin:0 auto;
    background-color: transparent;
}

.open-btn::after{
    display: none;
}

.closeRedPack{
    width: 42rpx;
    height: 42rpx;
    background-image: url('data:image/png;base64,');
    background-size: 100% 100%;
    position: absolute;
    top: 30rpx;
    left:30rpx;
}

@keyframes bounceIn {
    0%,
    100%,
    20%,
    40%,
    60%,
    80% {
        -webkit-transition-timing-function: cubic-bezier(0.215, .61, .355, 1);
        transition-timing-function: cubic-bezier(0.215, .61, .355, 1)
    }
    0% {
        opacity: 0;
        -webkit-transform: scale3d(.3, .3, .3);
        -ms-transform: scale3d(.3, .3, .3);
        transform: scale3d(.3, .3, .3);
        z-index: 100;
    }
    20% {
        -webkit-transform: scale3d(1.1, 1.1, 1.1);
        -ms-transform: scale3d(1.1, 1.1, 1.1);
        transform: scale3d(1.1, 1.1, 1.1);
        z-index: 100;
    }
    40% {
        -webkit-transform: scale3d(.9, .9, .9);
        -ms-transform: scale3d(.9, .9, .9);
        transform: scale3d(.9, .9, .9);
        z-index: 100;
    }
    60% {
        opacity: 1;
        -webkit-transform: scale3d(1.03, 1.03, 1.03);
        -ms-transform: scale3d(1.03, 1.03, 1.03);
        transform: scale3d(1.03, 1.03, 1.03);
        z-index: 100;
    }
    80% {
        -webkit-transform: scale3d(.97, .97, .97);
        -ms-transform: scale3d(.97, .97, .97);
        transform: scale3d(.97, .97, .97);
        z-index: 100;
    }
    100% {
        opacity: 1;
        -webkit-transform: scale3d(1, 1, 1);
        -ms-transform: scale3d(1, 1, 1);
        transform: scale3d(1, 1, 1);
        z-index: 100;
    }
}

/* 小红包 */
.redpack-icon{
    width: 56rpx;
    height: 75rpx;
    position: fixed;
    right: 20rpx;
    top: 190rpx;
    padding: 0;
    background-image: url('https://qncdn.playonwechat.com/hepulanhufu/index-redpack-icon.png');
    background-size: 100% 100%;
    animation: tada 1.5s infinite;
    border-radius: 0;
    background-color: transparent;
}

@keyframes tada {
  0% {
    -webkit-transform: scale3d(1, 1, 1);
    -ms-transform: scale3d(1, 1, 1);
    transform: scale3d(1, 1, 1)
  }
  10%,
  20% {
    -webkit-transform: scale3d(.9, .9, .9) rotate3d(0, 0, 1, -3deg);
    -ms-transform: scale3d(.9, .9, .9) rotate3d(0, 0, 1, -3deg);
    transform: scale3d(.9, .9, .9) rotate3d(0, 0, 1, -3deg)
  }
  30%,
  50%,
  70%,
  90% {
    -webkit-transform: scale3d(1.1, 1.1, 1.1) rotate3d(0, 0, 1, 3deg);
    -ms-transform: scale3d(1.1, 1.1, 1.1) rotate3d(0, 0, 1, 3deg);
    transform: scale3d(1.1, 1.1, 1.1) rotate3d(0, 0, 1, 3deg)
  }
  40%,
  60%,
  80% {
    -webkit-transform: scale3d(1.1, 1.1, 1.1) rotate3d(0, 0, 1, -3deg);
    -ms-transform: scale3d(1.1, 1.1, 1.1) rotate3d(0, 0, 1, -3deg);
    transform: scale3d(1.1, 1.1, 1.1) rotate3d(0, 0, 1, -3deg)
  }
  100% {
    -webkit-transform: scale3d(1, 1, 1);
    -ms-transform: scale3d(1, 1, 1);
    transform: scale3d(1, 1, 1)
  }
}